div class=\"c_row-item\">\n      <el-col class=\"label\" :span=\"4\">\n        <span v-if=\"datas[name].show\">{{ datas[name].title }}</span>\n        <span v-else>数量</span>\n      </el-col>\n      <el-col :span=\"19\" class=\"slider-box\">\n        <el-input\n          v-model=\"datas[name].val\"\n          type=\"number\"\n          placeholder=\"请输入数量\"\n          style=\"text-align: right; width: 350px\"\n          @blur=\"numberVal(datas[name].val)\"\n          @change=\"maxNum(datas[name].val)\"\n        />\n      </el-col>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'c_input_number',\n  props: {\n    name: {\n      type: String,\n    },\n    configData: {\n      type: null,\n    },\n    configNum: {\n      type: Number | String,\n      default: 'default',\n    },\n  },\n  data() {\n    return {\n      defaults: {},\n      sliderWidth: 0,\n      datas: this.configData[this.configNum],\n    };\n  },\n  mounted() {},\n  watch: {\n    configData: {\n      handler(nVal, oVal) {\n        this.datas = nVal[this.configNum];\n      },\n      immediate: true,\n      deep: true,\n    },\n  },\n  methods: {\n    numberVal(val) {\n      let r = /^\\+?[1-9][0-9]*$/;\n      if (!r.test(val)) {\n        this.datas[this.name].val = 3;\n      }\n    },\n    maxNum(val) {\n      if (val >= 50) {\n        this.datas[this.name].val = 50;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.numbox {\n  display: flex;\n  align-items: center;\n  margin: 20px 0 10px 0;\n\n  span {\n    width: 80px;\n    color: #999;\n  }\n}\n\n/* font-size 12px */\n.c_row-item {\n  width: 100%;\n}\n</style>\n"]}]}