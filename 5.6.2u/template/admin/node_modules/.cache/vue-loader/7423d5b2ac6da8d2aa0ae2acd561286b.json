 </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" v-db-click @click=\"memberAgreementSave\">保存</el-button>\n        </el-form-item>\n      </el-form>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport WangEditor from '@/components/wangEditor/index.vue';\nimport { agentAgreement, agentAgreementSave } from '@/api/user';\n\nexport default {\n  components: { WangEditor },\n  data() {\n    return {\n      ueConfig: {\n        autoHeightEnabled: false,\n        initialFrameHeight: 500,\n        initialFrameWidth: '100%',\n        UEDITOR_HOME_URL: '/UEditor/',\n        serverUrl: '',\n      },\n      agreement: {\n        content: '',\n        id: 0,\n      },\n      spinShow: false,\n    };\n  },\n  created() {\n    this.memberAgreement();\n  },\n  methods: {\n    getEditorContent(data) {\n      this.agreement.content = data;\n    },\n    memberAgreement() {\n      this.spinShow = true;\n      agentAgreement()\n        .then((res) => {\n          this.spinShow = false;\n          const { title, content, status, id } = res.data;\n          this.agreement.content = content;\n          this.agreement.id = id || 0;\n        })\n        .catch((err) => {\n          this.$message.error(err.msg);\n          this.spinShow = false;\n        });\n    },\n    // 保存\n    memberAgreementSave() {\n      agentAgreementSave(this.agreement)\n        .then((res) => {\n          this.$message.success('保存成功');\n          this.memberAgreement();\n        })\n        .catch((err) => {\n          this.$message.error(err.msg);\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n::v-deep .ivu-form-item-content {\n  line-height: unset !important;\n}\n</style>\n"]}]}