-------------------------------------------\n// | Author: CRMEB Team <admin@crmeb.com>\n// +----------------------------------------------------------------------\n\nimport { tableDelApi } from '@/api/common';\nexport function modalSure(delfromData) {\n  return new Promise((resolve, reject) => {\n    let content = `<p>确定要${delfromData.title}吗？</p>`;\n    if (!delfromData.info) {\n      delfromData.info = '';\n    }\n    const h = this.$createElement;\n    this.$msgbox({\n      title: '提示',\n      message: h('p', null, [h('div', null, `确定要${delfromData.title}吗？`), h('div', null, `${delfromData.info}`)]),\n      showCancelButton: true,\n      cancelButtonText: '取消',\n      confirmButtonText: '确定',\n      iconClass: 'el-icon-warning',\n      confirmButtonClass: 'btn-custom-cancel',\n    })\n      .then(() => {\n        if (delfromData.success) {\n          delfromData.success\n            .then(async (res) => {\n              resolve(res);\n            })\n            .catch((res) => {\n              reject(res);\n            });\n        } else {\n          tableDelApi(delfromData)\n            .then(async (res) => {\n              resolve(res);\n            })\n            .catch((res) => {\n              reject(res);\n            });\n        }\n      })\n      .catch(() => {});\n  });\n}\n\nexport function HandlePrice(num, type) {\n  let obj = [];\n  if (typeof num == 'number') {\n    obj = num.toString().split('.');\n  } else {\n    obj = num.split('.');\n  }\n  if (type) {\n    if (obj.length && obj[1]) {\n      return '.' + obj[1];\n    } else {\n      return '';\n    }\n  } else {\n    return obj[0];\n  }\n}\n"],"mappings":";;;;AAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,SAASA,WAAW,QAAQ,cAAc;AAC1C,OAAO,SAASC,SAASA,CAACC,WAAW,EAAE;EAAA,IAAAC,KAAA;EACrC,OAAO,IAAIC,OAAO,CAAC,UAACC,OAAO,EAAEC,MAAM,EAAK;IACtC,IAAIC,OAAO,2BAAAC,MAAA,CAAYN,WAAW,CAACO,KAAK,qBAAQ;IAChD,IAAI,CAACP,WAAW,CAACQ,IAAI,EAAE;MACrBR,WAAW,CAACQ,IAAI,GAAG,EAAE;IACvB;IACA,IAAMC,CAAC,GAAGR,KA