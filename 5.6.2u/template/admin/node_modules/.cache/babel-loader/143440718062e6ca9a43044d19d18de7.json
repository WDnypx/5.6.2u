ount","catch","err","$message","error","msg","pageChange","index","onchangeIsShow","row","_this2","id","status","_ref","_asyncToGenerator","_regenerator","m","_callee","w","_context","n","success","a","_x","apply","arguments","add","_this3","$modalForm","edit","_this4","setting","$router","push","path","$routeProStr","query","del","tit","num","_this5","delfromData","url","concat","method","ids","$modalSure","splice","length"],"sources":["src/pages/setting/ticket/index.vue"],"sourcesContent":["<template>\n  <!-- 商品-商品参数 -->\n  <div>\n    <el-card :bordered=\"false\" shadow=\"never\" class=\"ivu-mt\" :body-style=\"{ padding: 0 }\">\n      <div class=\"padding-add\">\n        <!-- 筛选条件 -->\n        <el-form\n          ref=\"specsFrom\"\n          inline\n          :model=\"specsFrom\"\n          :label-width=\"labelWidth\"\n          :label-position=\"labelPosition\"\n          @submit.native.prevent\n        >\n          <el-form-item label=\"打印机名称：\">\n            <el-input\n              v-model=\"specsFrom.keyword\"\n              placeholder=\"请输入打印机名称\"\n              class=\"form_content_width\"\n              clearable\n              @clear=\"specsSearchs\"\n              @change=\"specsSearchs\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"平台选择：\">\n            <el-select\n              class=\"form_content_width mr10\"\n              v-model=\"specsFrom.type\"\n              clearable\n              @clear=\"specsSearchs\"\n              @change=\"specsSearchs\"\n            >\n              <el-option v-for=\"(item, i) in optionsList\" :value=\"item.value\" :label=\"item.label\" :key=\"i\"></el-option>\n            </el-select>\n            <el-button type=\"primary\" @click=\"specsSearchs\">查询</el-button>\n          </el-form-item>\n        </el-form>\n      </div>\n    </el-card>\n    <el-card :bordered=\"false\" shadow=\"never\" class=\"ivu-mt mt16\">\n      <el-button type=\"primary\" v-db-click @click=\"add\">添加打印机</el-button>\n      <!-- 商品参数表格 -->\n      <el-table\n        :data=\"list\"\n        ref=\"table\"\n        class=\"mt25\"\n        :loading=\"loading\"\n        highlight-row\n        no-userFrom-text=\"暂无数据\"\n        no-filtered-userFrom-text=\"暂无筛选结果\"\n      >\n        <el-table-column label=\"ID\" min-width=\"50\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.id }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"打印机名称\" min-width=\"120\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.print_name }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"平台\" min-width=\"100\">\n          <template slot-scope=\"scope\">\n            <span v-if=\"scope.row.type == 1\">易联云</span>\n            <span v-if=\"scope.row.type == 2\">飞鹅云</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"应用账号\" min-width=\"100\">\n          <template slot-scope=\"scope\">\n            <span v-if=\"scope.row.type == 1\">{{ scope.row.yly_app_id }}</span>\n            <span v-if=\"scope.row.type == 2\">{{ scope.row.fey_user }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"打印联数\" min-width=\"80\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.times }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"创建时间\" min-width=\"130\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.add_time }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"打印开关\" min-width=\"100\">\n          <template slot-scope=\"scope\">\n            <el-switch\n              class=\"defineSwitch\"\n              :active-value=\"1\"\n              :inactive-value=\"0\"\n              v-model=\"scope.row.status\"\n              :value=\"scope.row.status\"\n              @change=\"onchangeIsShow(scope.row)\"\n              size=\"large\"\n              active-text=\"开启\"\n              inactive-text=\"关闭\"\n            >\n            </el-switch>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作\" fixed=\"right\" width=\"170\">\n          <template slot-scope=\"scope\">\n            <a @click=\"setting(scope.row.id)\">设计</a>\n            <el-divider direction=\"vertical\" />\n            <a @click=\"edit(scope.row.id)\">编辑</a>\n            <el-divider direction=\"vertical\" />\n            <a @click=\"del(scope.row, '删除打印机', scope.$index)\">删除</a>\n          </template>\n        </el-table-column>\n      </el-table>\n      <div class=\"acea-row row-right page\">\n        <pagination\n          v-if=\"total\"\n          :total=\"total\"\n          :page.sync=\"specsFrom.page\"\n          :limit.sync=\"specsFrom.limit\"\n          @pagination=\"getList\"\n        />\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex';\nimport { printList, printForm, printSetStatus } from '@/api/setting';\n\nexport default {\n  name: 'specs',\n  data() {\n    return {\n      grid: {\n        xl: 10,\n        lg: 10,\n        md: 12,\n        sm: 24,\n        xs: 24,\n      },\n      loading: false,\n      optionsList: [\n        {\n          value: '0',\n          label: '全部',\n        },\n        {\n          value: '1',\n          label: '易联云',\n        },\n        {\n          value: '2',\n          label: '飞鹅云',\n        },\n      ],\n      columns: [\n        {\n          title: 'ID',\n          key: 'id',\n          width: 80,\n        },\n        {\n          title: '打印机名称',\n          key: 'print_name',\n          minWidth: 100,\n        },\n        {\n          title: '平台',\n          slot: 'type',\n          minWidth: 100,\n        },\n        {\n          title: '应用账号',\n          slot: 'account',\n          minWidth: 100,\n        },\n        {\n        