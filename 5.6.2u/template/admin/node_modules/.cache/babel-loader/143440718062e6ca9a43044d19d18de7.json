n    setting(id) {\n      this.$router.push({\n        path: this.$routeProStr + '/setting/ticket/content',\n        query: { id: id },\n      });\n    },\n    // 删除\n    del(row, tit, num) {\n      let delfromData = {\n        title: tit,\n        num: num,\n        url: `system/ticket/del/${row.id}`,\n        method: 'DELETE',\n        ids: '',\n      };\n      this.$modalSure(delfromData)\n        .then((res) => {\n          this.$message.success(res.msg);\n          this.list.splice(num, 1);\n          if (!this.list.length) {\n            this.specsFrom.page = this.specsFrom.page == 1 ? 1 : this.specsFrom.page - 1;\n          }\n          this.getList();\n        })\n        .catch((err) => {\n          this.$message.error(err.msg);\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.input-add {\n  width: 250px;\n  margin-right: 14px;\n}\n</style>\n"],"mappings":";;;;;AA2HA,SAAAA,QAAA;AACA,SAAAC,SAAA,EAAAC,SAAA,EAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,IAAA;QACAC,EAAA;QACAC,EAAA;QACAC,EAAA;QACAC,EAAA;QACAC,EAAA;MACA;MACAC,OAAA;MACAC,WAAA,GACA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,EACA;MACAC,OAAA,GACA;QACAC,KAAA;QACAC,GAAA;QACAC,KAAA;MACA,GACA;QACAF,KAAA;QACAC,GAAA;QACAE,QAAA;MACA,GACA;QACAH,KAAA;QACAI,IAAA;QACAD,QAAA;MACA,GACA;QACAH,KAAA;QACAI,IAAA;QACAD,QAAA;MACA,GACA;QACAH,KAAA;QACAC,GAAA;QACAC,KAAA;MACA,GACA;QACAF,KAAA;QACAC,GAAA;QACAC,KAAA;MACA,GACA;QACAF,KAAA;QACAI,IAAA;QACAF,KAAA;MACA,GACA;QACAF,KAAA;QACAI,IAAA;QACAC,KAAA;QACAH,KAAA;MACA,EACA;MACAI,SAAA;QACAC,IAAA;QACAC,KAAA;QACAC,OAAA;QACAC,IAAA;MACA;MACAC,IAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA,EAAAC,aAAA,CAAAA,aAAA,KACA/B,QAAA;IACAgC,UAAA,WAAAA,WAAA;MACA,YAAAC,QAAA,GAAAC,SAAA;IACA;IACAC,aAAA,WAAAA,cAAA;MACA,YAAAF,QAAA;IACA;EAAA,EACA;EACAG,OAAA,WAAAA,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAC,YAAA,WAAAA,aAAA;MACA,KAAAhB,SAAA,CAAAC,IAAA;MACA,KAAAI,IAAA;MACA,KAAAS,OAAA;IACA;IAEA;IACAA,OAAA,WAAAA,QAAA;MAAA,IAAAG,KAA