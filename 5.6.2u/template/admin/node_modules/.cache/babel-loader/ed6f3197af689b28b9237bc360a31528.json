item\">\n      <span>直播间名称：</span>\n      {{ datas.name }}\n    </div>\n    <div class=\"item\">\n      <span>直播间ID：</span>\n      {{ datas.id }}\n    </div>\n    <div class=\"item box\">\n      <div class=\"box-item\" v-for=\"(item, index) in goodsList\" :key=\"index\">\n        <img :src=\"item.image\" alt=\"\" />\n        <i class=\"el-icon-close\" v-db-click @click=\"bindDelete(index)\" style=\"font-size: 20px\"></i>\n      </div>\n      <div class=\"upload-box\" v-db-click @click=\"modals = true\">\n        <i class=\"el-icon-picture-outline\" style=\"font-size: 24px\"></i>\n      </div>\n    </div>\n    <el-button type=\"primary\" style=\"width: 100%\" v-db-click @click=\"bindSub\">提交</el-button>\n    <el-dialog :visible.sync=\"modals\" title=\"商品列表\" class=\"paymentFooter\" width=\"1000px\">\n      <goods-list\n        ref=\"goodslist\"\n        @getProductId=\"getProductId\"\n        v-if=\"modals\"\n        :ischeckbox=\"true\"\n        :liveStatus=\"true\"\n      ></goods-list>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport goodsList from '@/components/goodsList';\nexport default {\n  name: 'add_goods',\n  components: {\n    goodsList,\n  },\n  props: {\n    datas: {\n      type: Object,\n      default: function () {\n        return {};\n      },\n    },\n  },\n  data() {\n    return {\n      modals: false,\n      goodsList: [],\n    };\n  },\n  methods: {\n    getProductId(data) {\n      this.goodsList = this.goodsList.concat(data);\n      this.$nextTick((res) => {\n        setTimeout(() => {\n          this.modals = false;\n        }, 300);\n      });\n    },\n    bindDelete(index) {\n      this.goodsList.splice(index, 1);\n    },\n    bindSub() {\n      let arr = [];\n      this.goodsList.map((el) => {\n        arr.push(el.product_id);\n      });\n      this.$emit('getData', arr);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.item {\n  margin-bottom: 10px;\n}\n.upload-box {\n  display: flex;\n  align-items: center;\n  just